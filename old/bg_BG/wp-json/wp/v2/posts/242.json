u044a\u0442 \u0438 \u0434\u0440\u0443\u0433\u0438\u0442\u0435 \u0445\u0438\u043c\u0438\u043a\u0430\u043b\u0438 \u0432 \u043f\u043b\u0443\u0432\u043d\u0438\u0442\u0435 \u0431\u0430\u0441\u0435\u0439\u043d\u0438. \u0417\u0430\u0442\u043e\u0432\u0430 \u043f\u0440\u0435\u0437 \u043b\u0435\u0442\u043d\u0438\u0442\u0435 \u043c\u0435\u0441\u0435\u0446\u0438 \u0435 \u0438\u0437\u043a\u043b\u044e\u0447\u0438\u0442\u0435\u043b\u043d\u043e \u0432\u0430\u0436\u043d\u043e \u0434\u0430 \u0441\u0435 \u043f\u043e\u043b\u0430\u0433\u0430\u0442 \u043c\u0430\u043b\u043a\u043e \u043f\u043e\u0432\u0435\u0447\u0435 \u0433\u0440\u0438\u0436\u0438 \u0437\u0430 \u043a\u043e\u0441\u0430\u0442\u0430 \u0441 \u0435\u043a\u0441\u0442\u0435\u043d\u0448\u044a\u043d\u0438, \u0437\u0430 \u0434\u0430 \u0441\u0435 \u0438\u0437\u0431\u0435\u0433\u043d\u0435 \u043d\u0435\u0439\u043d\u043e\u0442\u043e \u0438\u0437\u0441\u0443\u0448\u0430\u0432\u0430\u043d\u0435, \u0438\u0437\u0442\u044a\u043d\u044f\u0432\u0430\u043d\u0435 \u0438 \u0437\u0430\u0433\u0443\u0431\u0430 \u043d\u0430 \u0431\u043b\u044f\u0441\u044a\u043a. \u0420\u0430\u0437\u0431\u0438\u0440\u0430 \u0441\u0435 \u0437\u0430 \u0442\u043e\u0432\u0430 \u0441\u0430 \u043d\u0435 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u0438 \u043c\u043d\u043e\u0433\u043e \u0443\u0441\u0438\u043b\u0438\u044f \u0438\u043b\u0438 \u0441\u043a\u044a\u043f\u0438 \u043f\u0440\u043e\u0446\u0435\u0434\u0443\u0440\u0438. \u041d\u0430\u0448\u0430\u0442\u0430 \u043c\u0438\u043d\u0438 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u0430 \u043e\u0442 3 \u0441\u044a\u0432\u0435\u0442\u0430 \u0449\u0435 \u0432\u0438 \u043f\u043e\u043c\u043e\u0433\u043d\u0435 \u0435\u043a\u0441\u0442\u0435\u043d\u0448\u044a\u043d\u0438\u0442\u0435 \u0432\u0438 \u0434\u0430 \u0438\u0437\u0433\u043b\u0435\u0436\u0434\u0430\u0442 \u043a\u0440\u0430\u0441\u0438\u0432\u0438 \u043f\u0440\u0435\u0437 \u043d\u0430\u0431\u043b\u0438\u0436\u0430\u0432\u0430\u0449\u043e\u0442\u043e \u0433\u043e\u0440\u0435\u0449\u043e \u043b\u044f\u0442\u043e.<\/p>\n<h2>\u0411\u0435\u0437 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0438 \u0437\u0430 \u043a\u043e\u0441\u0430 \u0441\u044a\u0434\u044a\u0440\u0436\u0430\u0449\u0438 \u0430\u043b\u043a\u043e\u0445\u043e\u043b<\/h2>\n<p>\u041f\u044a\u0440\u0432\u0438\u044f\u0442 \u043d\u0438 \u0441\u044a\u0432\u0435\u0442 \u0435 \u043f\u0440\u0435\u0437 \u043b\u044f\u0442\u043e\u0442\u043e \u0434\u0430 \u0438\u0437\u0431\u044f\u0433\u0432\u0430\u0442\u0435 \u0441\u0442\u0438\u043b\u0438\u0437\u0438\u0440\u0430\u0449\u0438 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0438 \u0437\u0430 \u043a\u043e\u0441\u0430, \u043a\u043e\u0438\u0442\u043e \u0441\u044a\u0434\u044a\u0440\u0436\u0430\u0442 \u0430\u043b\u043a\u043e\u0445\u043e\u043b. \u041f\u043e\u0434\u043e\u0431\u043d\u0438 \u0441\u043f\u0440\u0435\u0439\u043e\u0432\u0435 \u0437\u0430\u0435\u0434\u043d\u043e \u0441\u044a\u0441 \u0441\u0438\u043b\u043d\u043e\u0442\u043e \u0441\u043b\u044a\u043d\u0446\u0435 \u0449\u0435 \u0438\u0437\u0441\u0443\u0448\u0430\u0442 \u0434\u043e\u043f\u044a\u043b\u043d\u0438\u0442\u0435\u043b\u043d\u043e \u043a\u043e\u0441\u0430\u0442\u0430 \u0432\u0438. \u0412\u044a\u043f\u0440\u0435\u043a\u0438, \u0447\u0435 \u0435\u043a\u0441\u0442\u0435\u043d\u0448\u044a\u043d\u0438\u0442\u0435 \u043a\u043e\u0438\u0442\u043e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u043c\u0435 (Art Forum Hair) \u0441\u0430 \u0438\u0437\u0440\u0430\u0431\u043e\u0442\u0435\u043d\u0438 \u043e\u0442 100% \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u0430 \u0447\u043e\u0432\u0435\u043a\u0430 \u043a\u043e\u0441\u0430 \u0441 \u043d\u0430\u0439-\u0432\u0438\u0441\u043e\u043a\u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e, \u0442\u0435 \u043d\u0435 \u043e\u0432\u043b\u0430\u0436\u043d\u044f\u0432\u0430\u0442 \u043a\u0430\u0442\u043e \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u0430\u0442\u0430 \u0432\u0438 \u043a\u043e\u0441\u0430 \u043e\u0442 \u0441\u043a\u0430\u043b\u043f\u0430. \u0417\u0430\u0442\u043e\u0432\u0430 \u0438\u0437\u043f\u043e\u043b\u0437\u0432\u0430\u0439\u0442\u0435 \u043e\u0432\u043b\u0430\u0436\u043d\u044f\u0432\u0430\u0449\u0438 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0438 \u0437\u0430 \u043a\u043e\u0441\u0430 \u0441 UV \u0437\u0430\u0449\u0438\u0442\u0430, \u0431\u0430\u043b\u0441\u0430\u043c\u0438 \u0438 \u043c\u0430\u0441\u043a\u0438, \u043a\u043e\u0438\u0442\u043e \u0441\u0430 \u043f\u043e\u0434\u0445\u043e\u0434\u044f\u0449\u0438 \u0438 \u0437\u0430 \u0432\u0430\u0448\u0430\u0442\u0430 \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u0430 \u043a\u043e\u0441\u0430.<\/p>\n<h2>\u0421\u0430\u043c\u043e \u0441\u0443\u0445\u0430 \u043a\u043e\u0441\u0430<\/h2>\n<p>\u0414\u043e\u0431\u0440\u0435 \u0435 \u0431\u044a\u0440\u0437\u043e \u0434\u0430 \u043f\u043e\u0434\u0441\u0443\u0448\u0430\u0432\u0430\u0442\u0435 \u043a\u043e\u0441\u0430\u0442\u0430 \u0441\u0438 \u0441 \u043a\u044a\u0440\u043f\u0430 \u0441\u043b\u0435\u0434 \u0432\u0441\u044f\u043a\u043e \u043c\u043e\u043a\u0440\u0435\u043d\u0435 \u0438\u043b\u0438 \u0430\u043a\u043e \u0435 \u0432\u044a\u0437\u043c\u043e\u0436\u043d\u043e \u0434\u0430 \u0438\u0437\u043f\u043e\u043b\u0437\u0432\u0430\u0442\u0435 \u0441\u0435\u0448\u043e\u0430\u0440. \u041d\u0435 \u043e\u0441\u0442\u0430\u0432\u0430\u0439\u0442\u0435 \u0435\u043a\u0441\u0442\u0435\u043d\u0448\u044a\u043d\u0438\u0442\u0435 \u043c\u043e\u043a\u0440\u0438 \u0434\u044a\u043b\u0433\u043e \u0432\u0440\u0435\u043c\u0435. \u041f\u0440\u0435\u043f\u043e\u0440\u044a\u0447\u0438\u0442\u0435\u043b\u043d\u043e \u0435 \u0434\u0430 \u0441\u043f\u043b\u0435\u0442\u0435 \u043a\u043e\u0441\u0430\u0442\u0430 \u0441\u0438 \u043d\u0430 \u043f\u043b\u0438\u0442\u043a\u0430 \u0438\u043b\u0438 \u0432 \u043d\u0438\u0441\u043a\u043e \u043a\u043e\u043a\u0447\u0435, \u0434\u043e\u043a\u0430\u0442\u043e \u0441\u0442\u0435 \u0432 \u043c\u043e\u0440\u0435\u0442\u043e \u0438\u043b\u0438 \u0431\u0430\u0441\u0435\u